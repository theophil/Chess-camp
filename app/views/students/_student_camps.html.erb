
<table class="table">
  <tr>
    <th>Name</th>
    <th>Start</th>
  </tr>

<% @camps.each do |camp| %>
  <tr>
    <td align="left"><%= link_to camp.name, camp_path(camp) %></td>
    <td align="center"><%= camp.start_date.strftime('%m/%d/%y') %></td>
  </tr>
<% end %>
</table>
<!-- Add the pagination using will_paginate gem -->
<span class="paginate">
<%= will_paginate camp_registrations, :previous_label => "Previous&nbsp;", :next_label => "&nbsp;Next" %>
</span>