<% unless @instructor_students.empty? %>
  <h3>&nbsp;</h3>
  <div class="panel clearfix">
    <h5>Your Upcoming Camps</h5>
    <ul class="side-nav">
      <% @instructor_students.each do |student| %>
        <li><%= link_to student.name, student_path(student) %></li>
      <% end %>
    </ul>
  </div>
<% end %>